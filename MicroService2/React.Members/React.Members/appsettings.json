{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MicroApplication": {
    "Fqan": "React.Members",
    "IngressBaseUrl": "<set-on-deployment>",
    "AppElementName": "mnc-members",
    "Widgets": [
      {
        "Name": "mnc-members-react-counter",
        "Description": ".",
        "Tags": [ "Control", "Dashboard" ]
      },
      {
        "Name": "mnc-members-member-detail-view",
        "Description": "A member detail view.",
        "Tags": [ "Control", "Dashboard" ]
      }
    ],
    "ScriptUrls": [ "static/js/main.js" ],
    "StyleUrls": []
  },
  "Registry": {
    "AuthHeader": "mnc-registry-api-key",
    "RegistryUrl": "<set-on-deployment>",
    "RegistrySecret": "<set-on-deployment>"
  },
  "AllowedHosts": "*"
}