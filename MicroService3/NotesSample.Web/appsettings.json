{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MicroApplication": {
    "Fqan": "NotesSample",
    "IngressBaseUrl": "<set-on-deployment>",
    "AppElementName": "mnc-notes",
    "Widgets": [
      {
        "Name": "mnc-notes-state-selector",
        "Description": "A drop down list displaying a list of states.",
        "Tags": [ "Control", "Forms" ]
      }
    ],
    "ScriptUrls": [ "runtime.js", "polyfills.js", "vendor.js", "main.js" ],
    "StyleUrls": [ "styles.css" ]
  },
  "Registry": {
    "AuthHeader": "mnc-registry-api-key",
    "RegistryUrl": "<set-on-deployment>",
    "RegistrySecret": "<set-on-deployment>"
  },
  "AllowedHosts": "*"
}